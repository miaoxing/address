{
  "name": "miaoxing/address",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Address Plugin",
  "keywords": [
    "miaoxing",
    "address"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.6.3",
    "miaoxing/region": "^0.2.6"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Address\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Address\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 0.1.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^8.1.0"
  }
}
