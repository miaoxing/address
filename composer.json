{
  "name": "miaoxing/address",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Address Plugin",
  "keywords": [
    "miaoxing",
    "address"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.2.4",
    "miaoxing/region": "^0.1.1"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Address\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Address\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  }
}
