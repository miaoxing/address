{
  "name": "miaoxing/address",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Address Plugin",
  "keywords": [
    "miaoxing",
    "address"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.10.6",
    "miaoxing/region": "^0.2.40"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Address\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Address\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 9.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 3.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^9.2.0"
  },
  "scripts": {
    "post-update-cmd": [
      "@php wei g:auto-completion"
    ]
  }
}
