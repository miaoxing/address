{
  "name": "miaoxing/address",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Address Plugin",
  "keywords": [
    "miaoxing",
    "address"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.3.0",
    "miaoxing/region": "^0.1.11"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Address\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Address\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    },
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 6.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^6.4.0"
  }
}
